<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Configuration status="trace" moniterInternal="30">
    <Appenders>
<Console name="STDOUT" target="SYSTEM_OUT">
			<PatternLayout
				pattern="%d [%t] [%-5level] [%class{2}] [%marker{1}] - %msg%n" />
		</Console>
		
    </Appenders>
    <Loggers>
        <Root level="DEBUG">
            <AppenderRef ref="STDOUT"/>
        </Root>
    </Loggers>
</Configuration>